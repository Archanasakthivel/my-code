#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int n;
 int *arr,sum=0,i,j;
 scanf("%d",&n);
 arr=(int *) malloc(n*sizeof(int));
 for(i=0;i<n;i++)
 {
    scanf("%d ",arr+i);
 }  
 for(i=0;i<n;i++)
 {
     sum=sum+*(arr+i);
 }  
 printf("%d",sum);
    return 0;
}

