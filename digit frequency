#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
char s[1000],b[10]={0};
int i;
scanf(" %[^\n]",s);
for(i=0;s[i]!='\0';i++)
{
    switch(s[i])
    {
case '0':
    b[0]=b[0]+1;
break;

case '1':
    b[1]=b[1]+1;
break;

case '2':
    b[2]=b[2]+1;
break;

case '3':
    b[3]=b[3]+1;
break;
case '4':

    b[4]=b[4]+1;
    break;

case '5':

    b[5]=b[5]+1;
    break;

case '6':

    b[6]=b[6]+1;

break;
case '7':

    b[7]=b[7]+1;

break;
case '8':

    b[8]=b[8]+1;

break;
case '9':
    b[9]=b[9]+1;
break;
default:
continue;
}
}
for(i=0;i<10;i++)
{
    printf("%d ",b[i]);
}

    return 0;
}
